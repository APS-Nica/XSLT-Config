    <!--Start of replace_sab template-->
    <xsl:template name="replace_sab">
        <!-- with string s, replace substring a by string b -->
        <!-- s, a and b are parameters determined upon calling  -->
        <xsl:param name="s"/>
        <xsl:param name="a"/>
        <xsl:param name="b"/>
        <xsl:choose>
            <xsl:when test="contains($s,$a)">
                <xsl:value-of select="substring-before($s,$a)"/>
                <xsl:copy-of select="$b"/>
                <xsl:call-template name="replace_sab">
                    <xsl:with-param name="s" select="substring-after($s,$a)"/>
                    <xsl:with-param name="a" select="$a"/>
                    <xsl:with-param name="b" select="$b"/>
                </xsl:call-template>
            </xsl:when>
            <xsl:otherwise>
                <xsl:value-of select="$s"/>
            </xsl:otherwise>
        </xsl:choose>
    </xsl:template>
    <!-- End of Replace_sab template -->
    <!--Start of Remove markup -->
    <xsl:template name="remove-markup">
        <xsl:param name="string"/>
        <xsl:choose>
            <xsl:when test="contains($string, '&lt;')">
                <xsl:value-of select="substring-before($string, '&lt;')"/>
                <!-- recursive call -->
                <xsl:call-template name="remove-markup">
                    <xsl:with-param name="string" select="substring-after($string, '&gt;')"/>
                </xsl:call-template>
            </xsl:when>
            <xsl:otherwise>
                <xsl:value-of select="$string"/>
            </xsl:otherwise>
        </xsl:choose>
    </xsl:template>
    <!--End of Start of Remove markup -->
    <!-- Start of substringafterlast template -->
    <xsl:template name="substring-after-last">
        <!--Start of Define Parameters-->
        <xsl:param name="input"/>
        <xsl:param name="substr"/>
        <!--End of define parameters -->
        <!-- Example Here -->
        <!--
		                                    <xsl:variable name="Citysub"><xsl:call-template name="substring-after-last"><xsl:with-param name="input" select="$Country_City"/><xsl:with-param name="substr" select="'-'"/></xsl:call-template></xsl:variable>
		-->
        <!--Example Here -->
        <!-- Extract the string which comes after the first occurrence -->
        <!--Start of Create a temporal variable to extract  value with delimiter substr from value input -->
        <xsl:variable name="temp" select="substring-after($input,$substr)"/>
        <xsl:choose>
            <!-- If it still contains the search string the recursively process -->
            <xsl:when test="$substr and contains($temp,$substr)">
                <!-- Start of We call the template again until theere is no delimiter available-->
                <xsl:call-template name="substring-after-last">
                    <xsl:with-param name="input" select="$temp"/>
                    <xsl:with-param name="substr" select="$substr"/>
                </xsl:call-template>
                <!-- End of We call the template again until theere is no delimiter available-->
            </xsl:when>
            <xsl:otherwise>
                <xsl:value-of select="$temp"/>
            </xsl:otherwise>
        </xsl:choose>
        <!--End of Create a temporal variable to extract  value with delimiter substr from value input -->
    </xsl:template>
    <!-- End of substringafterlast template -->
</xsl:stylesheet>
