URL ON CA: https://recruiting.adp.com/srccsh/public/ws_req_feed.guid?c=1020032&d=External2.0&o=true

-->
    <!-- End of comments for XSL -->
    <!--Start of Job Template -->
    <xsl:template match="*">
        <!-- Define here global variables for formatting -->
        <xsl:variable name="smallcase" select="'abcdefghijklmnopqrstuvwxyz'"/>
        <xsl:variable name="uppercase" select="'ABCDEFGHIJKLMNOPQRSTUVWXYZ'"/>
        <!--End of Define here global variables for formatting -->
        <Batch>
            <!--Start of Here we create the headers -->
            <xsl:call-template name="create_header_node"/>
            <!--End of Here we create the headers -->
            <xsl:for-each select="JOB">
                <Job>
                    <!-- Start of Here we create the defaults for each job -->
                    <xsl:call-template name="set_job_defaults"/>
                    <!-- End of Here we create the defaults for each job -->
                    <!--Start of Variables-->
                    <xsl:variable name="NormalizedPostalCode" select="normalize-space(LOCATIONZIP)"/>
                    <xsl:variable name="NormalizedCity" select="normalize-space(LOCATIONCITY)"/>
                    <xsl:variable name="NormalizedState" select="normalize-space(LOCATIONSTATE)"/>
                    <xsl:variable name="NormalizedCountry" select="normalize-space(LOCATIONCOUNTRY)"/>
                    <xsl:variable name="NormalizedJobID" select="normalize-space(REQNUMBER)"/>
                    <xsl:variable name="NormalizedJobTitle" select="JOBTITLE"/>
                    <xsl:variable name="NormalizedEmployeeType" select="normalize-space(FULLTIME)"/>
                    <xsl:variable name="NormalizedApplyURL" select="normalize-space(JOBLINK)"/>
                    <xsl:variable name="NormalizedDescription" select="JOBBODY"/>
                    <xsl:variable name="Desc1">
                        <xsl:call-template name="replace_sab">
                            <xsl:with-param name="s" select="$NormalizedDescription"/>
                            <xsl:with-param name="a" select="'&#10;'"/>
                            <xsl:with-param name="b" select="'&lt;br&gt;'"/>
                        </xsl:call-template>
                    </xsl:variable>
                    <!--End of Variables-->
                    <!--Start Of Most Common Fields Define Here -->
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBUserJobID</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="concat('~TN',$NormalizedJobID)"/>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBDisplayJobID</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$NormalizedJobID"/>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBJobTitle</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$NormalizedJobTitle"/>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBPostalCode</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$NormalizedPostalCode"/>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBCity</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$NormalizedCity"/>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBState</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$NormalizedState"/>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBCountry</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:choose>
                                <xsl:when test="contains($NormalizedCountry,'United States of America')">US</xsl:when>
                                <xsl:when test="contains($NormalizedCountry,'United States')">US</xsl:when>
                                <xsl:when test="contains($NormalizedCountry,'USA')">US</xsl:when>
                                <xsl:otherwise>
                                    <xsl:value-of select="normalize-space($NormalizedCountry)"/>
                                </xsl:otherwise>
                            </xsl:choose>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBApplyURL</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$NormalizedApplyURL"/>
                        </xsl:attribute>
                    </xsl:element>
                    <xsl:element name="Field">
                        <xsl:attribute name="name">CBDescription</xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$Desc1"/>
                        </xsl:attribute>
                    </xsl:element>
                    <!--End Of Of Most Common Fields Define Here -->
                    <!-- Start of Define categories for this XSL here-->
                    <xsl:call-template name="set_job_Category">
                        <xsl:with-param name="ParamTitle" select="$NormalizedJobTitle"/>
                    </xsl:call-template>
                    <!-- End of Define categories for this XSL here-->
                    <!--Start  of Define Employee Type for this XSL here -->
                    <xsl:call-template name="set_job_EmployeeType">
                        <xsl:with-param name="ParamEmployeeType" select="$NormalizedEmployeeType"/>
                    </xsl:call-template>
                    <!--End of Define Employee Type for this XSL here -->
                    <!-- EOJ-->
                </Job>
                <!-- End of CB Job node -->
            </xsl:for-each>
            <!-- End of BM job node -->
        </Batch>
        <!-- End of Batch -->
    </xsl:template>
