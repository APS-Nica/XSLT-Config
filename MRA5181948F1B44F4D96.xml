    <!--Start of template for headers -->
    <xsl:template name="create_header_node">
        <Header>
            <Field name="CBVendorID" value="ED3N7YJ5XV7DJZK2DMWM"/>
            <Field name="CBOrigin" value="RecruitingADP-CustomXSL"/>
            <Field name="CBReplace" value="Yes"/>
            <Field name="CBReplyTo" value="dpisupport@careerbuilder.com"/>
            <Field name="CBEmailPref" value="EmailOnlyIfErrors"/>
        </Header>
    </xsl:template>
    <!--End of Template for headers-->
    <!-- Start of Template for defaults -->
    <xsl:template name="set_job_defaults">
        <Field name="CBAction" value="ADD"/>
        <Field name="CBProductID" value="JCPRV0"/>
        <Field name="CBContactCompany" value="Sapp Bros. Truck Stops"/>
    </xsl:template>
    <!-- End of Template for defaults-->
    <!-- Start of comments for XSL -->
